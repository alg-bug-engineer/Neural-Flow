# Copy this file to .env and fill your local secrets.
# cp .env.example .env

# Local integration config file path (optional)
FEISHU_CONFIG_PATH=./config/feishu_config.json

# Logging
LOG_LEVEL=INFO
LOG_DB_PATH=./data/system_logs.db

# Cortex (Kimi / Moonshot) - env has priority over json config
KIMI_API_KEY=
KIMI_BASE_URL=https://api.moonshot.cn/v1
KIMI_MODEL=kimi-k2-turbo-preview

# Iris (Jimeng / Volcano) - env has priority over json config
JIMENG_AK=
JIMENG_SK=
JIMENG_REQ_KEY=jimeng_t2i_v40

# Legacy assistant compatibility
JIMENG_TOKEN=

# Feishu app credentials (optional env override)
FEISHU_APP_ID=
FEISHU_APP_SECRET=
FEISHU_BITABLE_APP_TOKEN=
FEISHU_BITABLE_TABLE_ID=
FEISHU_ROOT_FOLDER_TOKEN=
FEISHU_RECEIVE_ID=
